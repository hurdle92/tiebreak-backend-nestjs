{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/tiebreak/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/v2/(.*)",
      "dest": "apps/crm/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/docs/(.*\\.css)",
      "dest": "/docs/$1",
      "headers": { "Content-Type": "text/css" }
    },
    {
      "src": "/docs/(.*\\.js)",
      "dest": "/docs/$1",
      "headers": { "Content-Type": "application/javascript" }
    },
    { "src": "/docs/(.*)", "dest": "/docs/$1" }
  ]
}
